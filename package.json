{
  "name": "wheeldogg-blog",
  "version": "1.0.0",
  "type": "module",
  "description": "Personal blog of Wheeldogg - Music, Software, Nature, Fitness & Visual Arts",
  "scripts": {
    "dev": "npm-run-all clean --parallel dev:*",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:vite": "vite",
    "build": "npm-run-all clean build:vite build:11ty",
    "build:11ty": "eleventy",
    "build:vite": "vite build",
    "clean": "rimraf dist",
    "preview": "npm run build && npx serve dist"
  },
  "dependencies": {
    "alpinejs": "^3.14.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@web-alchemy/eleventy-plugin-twig": "^0.1.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.0.0",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
